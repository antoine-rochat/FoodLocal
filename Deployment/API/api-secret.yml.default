apiVersion: v1
kind: Secret
metadata:
  name: api-secret
type: Opaque
data:
  PORT: yourPortInBase64
  NODE_ENV: yourNodeEnvInBase64
  MONGODB_PORT_PROD: yourDbPortInBase64=
  MONGODB_HOST_PROD: yourDbHostInBase64
  MONGODB_DBNAME_PROD: yourDbNameInBase64
  JWT_SECRET: yourJWTSecretInBase64
  REPLICA_SET_NAME_PROD: yourReplicatSetNameInBase64